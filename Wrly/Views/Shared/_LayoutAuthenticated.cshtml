<!doctype html>
<!--[if IE 8]><html class="ie8 lt-ie10"><![endif]-->
<!--[if IE 9]><html class="ie9 lt-ie10"><![endif]-->
<!--[if gt IE 9]><!-->
<html lang="en">
<!--<![endif]-->
<head>
    <link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta http-equiv="cleartype" content="on">
    <meta name="HandheldFriendly" content="True">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">
    @if (ViewBag.Title == null)
    {
        <title>sklative - the skill's network</title>
        <meta property="og:title" content="sklative - the skill's network" />
    }
    else
    {
        <title>@ViewBag.Title</title>
        <meta property="og:title" content="@ViewBag.Title" />
    }
    @{Html.RenderPartial("_StaticMetas");}
    @if (ViewBag.Description != null)
    {
        <meta property="og:description" content="@ViewBag.Description" />
        <meta name="description" content="@ViewBag.Description" />
    }
    @if (ViewBag.Keywords != null)
    {
        <meta name="keywords" content="@ViewBag.Keywords" />
    }
    @if (ViewBag.AddNoFollow == true)
    {
        <meta name="robots" content="noindex, nofollow" />
    }
    <link rel="shortcut icon" href="/favicon.ico" />

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Scripts/jquery-ui.min.js"></script>
    @Scripts.Render("~/cdn/_AuthLayoutScripts")
    @Styles.Render("~/cdn/_LayoutStyles")
    @Styles.Render("~/cdn/_AuthLayoutStyles")
    @RenderSection("_HeadStyles", false)

</head>
<body>
    <!-- Google Tag Manager (noscript) -->
    <noscript>
        <iframe src="https://www.googletagmanager.com/ns.html?id=GTM-KJN2QN9"
                height="0" width="0" style="display:none;visibility:hidden"></iframe>
    </noscript>
    <!-- End Google Tag Manager (noscript) -->
    <div class="floated-container new-message">
        <button class="remove" data-val-entity="floated-container" data-val-quick-action="dismiss">X</button>
        <div class="container">

        </div>
    </div>
    <main id="panel" class="panel">
        <header class="panel-header">
            <div class="container">
                <div class="header-control-container">
                    <div class="statistics-container">
                        <div class="left" id="profile-statistics">
                            <div class="statistic-launcher header-face">
                                <div class="btn-group">
                                    <div data-ajax="true" data-ajax-url="/account/usermenu" data-container="parent" class="dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                        <div class="header-face-head">
                                            <img src="@Html.UserNamePhoto()" />
                                        </div>
                                    </div>
                                    <ul class="dropdown-menu quick-view-wrapper action-list"></ul>
                                </div>
                            </div>
                            <span class="statistic-launcher messages">
                                <span data-action-request class="dropdown-toggle">
                                    <span>
                                        <i class="icon icon-messages"></i>
                                        <span class="count">
                                        </span>
                                    </span>
                                </span>
                                <div class="dropdown-menu max-container quick-view-wrapper">
                                    <div class="container">
                                        <div class="header">
                                            <div><a href="/conversations">See all</a></div>
                                        </div>
                                        <ul id="messages"></ul>
                                        <div class="footer">
                                            <button class="link-alike" data-val-quick-action="acknowledged" data-val-entity="conversations">Mark all aknowledged</button> <span class="verticle-divider">|</span> <button data-val-entity="conversations" data-val-quick-action="new" class="link-alike">Start new</button>
                                        </div>
                                    </div>
                                </div>
                            </span>
                            <span class="statistic-launcher notifications">
                                <span data-action-request class="dropdown-toggle">
                                    <span>
                                        <i class="icon icon-notifications"></i>
                                        <span class="count">
                                        </span>
                                    </span>
                                </span>
                                <div class="dropdown-menu max-container quick-view-wrapper">
                                    <div class="container">
                                        <div class="header">
                                            <div><a href="/notifications">See all</a></div>
                                        </div>
                                        <ul id="notifications"></ul>
                                        <div class="footer">
                                            <button class="link-alike" data-val-quick-action="acknowledged" data-val-entity="notifications">Mark all aknowledged</button>
                                        </div>
                                    </div>
                                </div>
                            </span>
                            <span class="statistic-launcher requests">
                                <span data-action-request class="dropdown-toggle">
                                    <span>
                                        <i class="icon icon-requests"></i>
                                        <span class="count">
                                        </span>
                                    </span>
                                </span>
                                <div class="dropdown-menu max-container quick-view-wrapper">
                                    <div class="container">
                                        <div class="header">
                                            <div><a href="/requests">See all</a></div>
                                        </div>
                                        <ul id="requests"></ul>
                                        <div class="footer">
                                            <a class="link-alike" href="/configurations#network">Settings</a>
                                        </div>
                                    </div>
                                </div>
                            </span>
                            <span class="statistic-launcher groups">
                                <span>
                                    <span>
                                        <a href="/groups">
                                            <i class="icon icon-group"></i>
                                            <span class="statistics-title">Groups</span>
                                        </a>
                                    </span>
                                </span>
                            </span>
                        </div>
                        <div class="right">
                            @using (Html.BeginForm("Index", "Search", FormMethod.Post, new { @id = "____frmMainSearch" }))
                            {
                                <input type="hidden" name="Url" id="____hdnUrl" />
                                <input type="hidden" name="EntityID" id="____hdnEntityID" />
                                <input type="hidden" name="EntityType" id="____hdnEntityType" />
                                <div class="search-container">
                                    @Html.AntiForgeryToken()
                                    <i class="icon icon-search"></i>
                                    <input type="text" class="big-search" name="Keyword" placeholder="person, organization or group name" />
                                    <div class="search-content">

                                    </div>
                                </div>
                            }
                            <a href="/feed" class="title"><img src="data:image/png;base64," class="title-brand" alt="sklative - logo" /></a>
                        </div>
                    </div>
                </div>
            </div>
        </header>
        @RenderSection("_HeaderOfProfile", false)
        <section class="box-container">
            <section class="box">
                <div class="bg-layer"></div>
                @RenderBody()
                <div class="alert-success-popabble">
                </div>
                <div class="alert-error-popabble">
                </div>
            </section>
        </section>
    </main>
    <footer class="panel-footer">
        @{Html.RenderPartial("_Footer");}
        @{Html.RenderPartial("_Analytics");}
    </footer>
    @RenderSection("_FooterScripts", false)
    <div class="progress-bar color0" id="gloablWaiting">
        <span></span>
    </div>
</body>
</html>
