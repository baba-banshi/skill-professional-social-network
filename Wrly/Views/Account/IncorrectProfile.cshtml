@{
    ViewBag.Title = "Invalid profile";
    Layout = "~/Views/Shared/_LayoutWizard.cshtml";
}

<div class="string join-container">
    <div class="string-wrapper">
        <div class="content-container">
            <div class="reg-content-wrapper">
                <div class="wizard-form-container form-container">
                    <div class="wizard-form-wrapper">
                        <p>
                            <div class="alert alert-error">
                                The link seems to be invalid, tempered or broken, we are not able to understand the requested profile, please try with a fresh link or request or <a href="/interaction/feedback">report </a> this error or below are some possible reasons you may check.
                                <ul class="default-list">
                                    <li>The requested profile has been moved to some other name</li>
                                    <li>You have entered some extra or less characters in profile name</li>
                                    <li>There is a space at the end of in url</li>
                                    <li>The profile could never exist with us and this is just a manual url created</li>
                                </ul>
                            </div>
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
